fn main() {
    var result = "";
    var count = 0;

    // String operations benchmark
    for i in 0..<5000 {
        var num = rand(1);
        var str_num = toString(num);

        // String concatenation
        result = result + str_num + ",";

        // String length operations
        count = count + #result;
    }

    print(count);
    print(result);
    print(#result);
}
fn main() -> void {
    let ref_var: ref[int] = new(42);

    // Valid: explicit comparison with nil
    if ref_var != nil {
        print("Reference is not null");
    }

    // Valid: explicit comparison with self
    if ref_var == ref_var {
        print("Reference is equal to itself");
    }

    // Valid: explicit comparison
    let is_null: bool = ref_var == nil;
    print(is_null);
}
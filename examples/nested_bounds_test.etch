fn main() -> void {
    // Test nested array bounds checking
    let matrix: array[array[int]] = [[1, 2], [3, 4, 5], [6]];
    
    // Valid accesses
    let a: int = matrix[0][1];  // Should be 2
    let b: int = matrix[1][2];  // Should be 5  
    let c: int = matrix[2][0];  // Should be 6
    
    // Invalid access - this should fail at compile time
    let d: int = matrix[1][3];  // Out of bounds - inner array [3, 4, 5] only has indices 0, 1, 2
}
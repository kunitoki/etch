fn main() -> void {
    let numbers: array[int] = [1, 2, 3, 4, 5];
    let first: int = numbers[0];
    let slice: array[int] = numbers[1:3];
    let tail: array[int] = numbers[2:];
    let head: array[int] = numbers[:3];

    print(first);  // Expected: 1
    print(#slice); // Expected: 2
    print(#tail);  // Expected: 3
    print(#head);  // Expected: 3
}
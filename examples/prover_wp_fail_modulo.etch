// Test: WP should catch modulo by zero
// This test should FAIL at compile time

fn unsafeModulo(a: int, b: int) -> int {
  // WP should infer: requires b != 0
  return a % b;
}

fn main() -> void {
  // This should fail: b = 0
  let result = unsafeModulo(42, 0);
  print("Result: " + toString(result));
}

// Test that match expressions used as statements with non-void return require discard

fn getString() -> string {
  return "hello";
}

fn main() {
  let x: option[int] = some(42);

  // Match expression used as statement with non-void return type
  // Since the match result (string) is not used, it should require discard
  match x {
    some(num) => {
      getString();
    }
    none => {
      "no value";
    }
  };
}

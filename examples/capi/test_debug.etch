// Simple test script for debugging C binaries
// This script demonstrates debugging features like breakpoints,
// stepping, and variable inspection

fn factorial(n: int) -> int {
    if n <= 1 {
        return 1;
    }
    return n * factorial(n - 1);
}

fn main() -> void {
    let x: int = 5;
    let res: int = factorial(x);
    print(res);

    let y: int = 10;
    let z: int = x + y;
    print(z);
}
